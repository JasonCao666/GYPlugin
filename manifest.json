{
  "manifest_version": 2,
  "version": "1.0.2",
  "name": "GY Plugin",
  "description": "tasks",
  "browser_action": {
    "default_icon": "1.png",
    "default_popup":"popup.html"
  },
  "permissions": [
    "http://*/*","https://*/*"
  ],
  "background": {
    "scripts":["jquery-3.3.1.js","background.js"],
    "persistent": true
  },
  "content_scripts": [
    {
      "matches":["https://www.myed.ed.ac.uk/*"],
      "js": ["jquery-3.3.1.js","processContent.js"]
    }
  ]
}