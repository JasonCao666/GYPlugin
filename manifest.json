{
  "manifest_version": 2,
  "version": "1.0.2",
  "name": "GY Plugin",
  "description": "tasks",
  "browser_action": {
    "default_icon": "1.png",
    "default_popup":"popup.html"
  },
  "permissions": [
    "https://login.taobao.com/*"
  ],
  "background": {

  },
  "content_scripts": [
    {
      "matches": ["https://login.taobao.com/*"],
      "js": ["taobao_login.js"],
      "run_at": "document_end",
      "all_frames": true
    }
  ]
}